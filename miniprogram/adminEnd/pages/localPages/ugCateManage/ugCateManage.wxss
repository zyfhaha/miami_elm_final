page {
  padding-bottom: 90rpx;
}
.drag_container {
  height: calc(100vh - 90rpx);
}
.btm_tool {
  border-top: 1rpx solid #ccc;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  background-color: var(--themeColor);
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 111;
  box-sizing: border-box;
}
.btm_save {
  background-color: var(--themeColor);
  color: white;
  font-size: 30rpx;
  font-weight: 600;
}
.view_hover {
  background: #dedede;
}
.bottom_btn {
  border-top: 3rpx solid #ccc;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  display: flex;
  z-index: 111;
  justify-content: center;
  align-items: center;
}
.bottom_btn .sort_cate {
  display: flex;
  width: 100%;
  height: 100%;
  background-color: white;
  color: var(--themeColor);
  font-size: 30rpx;
  font-weight: 600;
  justify-content: center;
  align-items: center;
}
.bottom_btn .add_cate {
  display: flex;
  width: 100%;
  height: 100%;
  background-color: var(--themeColor);
  color: white;
  font-size: 30rpx;
  font-weight: 600;
  justify-content: center;
  align-items: center;
}
.input_dialog .input_warp {
  margin-top: 30rpx;
  border-bottom: 2rpx solid black;
}
.input_dialog .input_warp .input_text {
  text-align: start;
}
