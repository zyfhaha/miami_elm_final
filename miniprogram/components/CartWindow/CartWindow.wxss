.actionSheet {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
}
.actionSheet .master {
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0.2;
}
.actionSheet .content {
  border-radius: 20rpx;
  background-color: white;
  position: relative;
  top: -20%;
}
.actionSheet .content .cart_title_warp {
  display: flex;
  padding: 30rpx;
  justify-content: space-between;
}
.actionSheet .content .cart_title_warp .cart_title_text {
  font-size: 30rpx;
  font-weight: bold;
}
.actionSheet .content .cart_title_warp .iconfont.icon-trash_can {
  font-size: 35rpx;
  color: var(--themeColor);
}
.actionSheet .content .cart_window_content .goods_item {
  padding: 0 30rpx;
}
.actionSheet .content .cart_window_content .goods_item .goods_info_warp {
  position: relative;
  padding: 15rpx 0;
  background-color: white;
  display: flex;
  border-bottom: 3rpx solid #f6f6f6;
}
.actionSheet .content .cart_window_content .goods_item .goods_info_warp .goods_pic_warp .goods_pic {
  width: 150rpx;
  height: 150rpx;
  flex: none;
}
.actionSheet .content .cart_window_content .goods_item .goods_info_warp .goods_basic_info_warp {
  width: 100%;
  margin-left: 15rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.actionSheet .content .cart_window_content .goods_item .goods_info_warp .goods_basic_info_warp .goods_name {
  font-size: 25rpx;
  font-weight: bold;
  width: 350rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.actionSheet .content .cart_window_content .goods_item .goods_info_warp .goods_basic_info_warp .bottom_warp {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.actionSheet .content .cart_window_content .goods_item .goods_info_warp .goods_basic_info_warp .bottom_warp .goods_price {
  flex: 1.2;
  font-size: 30rpx;
  color: var(--themeColor);
}
.actionSheet .content .cart_window_content .goods_item .goods_info_warp .goods_basic_info_warp .bottom_warp .operation_warp {
  padding-right: 35rpx;
  flex: 1;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.actionSheet .content .cart_window_content .goods_item .goods_info_warp .goods_basic_info_warp .bottom_warp .operation_warp .reduce_btn {
  width: 45rpx;
  height: 45rpx;
  font-size: 40rpx;
  color: white;
  text-align: center;
  line-height: 45rpx;
  background-color: var(--themeColor);
  border-radius: 100%;
  box-shadow: 5rpx 5rpx 8rpx 1rpx rgba(0, 0, 0, 0.164);
}
.actionSheet .content .cart_window_content .goods_item .goods_info_warp .goods_basic_info_warp .bottom_warp .operation_warp .goodsNum {
  margin-left: auto;
}
.actionSheet .content .cart_window_content .goods_item .goods_info_warp .goods_basic_info_warp .bottom_warp .operation_warp .add_btn {
  margin-left: auto;
  width: 45rpx;
  height: 45rpx;
  font-size: 40rpx;
  color: white;
  text-align: center;
  line-height: 45rpx;
  background-color: var(--themeColor);
  border-radius: 100%;
  box-shadow: 5rpx 5rpx 8rpx 1rpx rgba(0, 0, 0, 0.164);
}
